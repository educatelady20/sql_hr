SELECT *
FROM employees


SELECT *
FROM employees
WHERE job_title = 'Social Worker'


SELECT *
FROM employees
WHERE job_title = 'Staff Scientist'


SELECT *
FROM employees
WHERE job_title = 'Financial Advisor'


SELECT *
FROM employees
WHERE job_title = 'VP Marketing'


SELECT *
FROM employees
WHERE job_title = 'Food Chemist'



SELECT *
FROM employees
WHERE job_title = 'Pharmacist'


SELECT *
FROM employees
WHERE job_title = 'General Manager'


SELECT *
FROM employees
WHERE job_title = 'Geologist II'


SELECT *
FROM employees
WHERE job_title = 'Cost Accountant'



SELECT *
FROM employees
WHERE office_id = 3
ORDER BY first_name



SELECT *
FROM employees
WHERE salary < 50000 
ORDER BY first_name


SELECT *
FROM employees
WHERE salary >= 50000 
ORDER BY job_title


SELECT 
    first_name,
    last_name,
    job_title,
    salary
FROM employees
WHERE salary >= 50000 
ORDER BY salary desc



SELECT 
    first_name,
    last_name,
    job_title,
    salary
FROM employees
WHERE salary <= 50000 
ORDER BY salary desc


SELECT *
FROM employees
WHERE last_name REGEXP '^M'


SELECT *
FROM employees
WHERE salary > 100000 AND reports_to = '37270'



SELECT *
FROM employees
WHERE salary < 100000 AND reports_to = '37270' AND office_id = '2'


SELECT *
FROM employees
WHERE salary < 100000 AND reports_to = '37270' AND office_id = '5'
ORDER BY salary ASC
